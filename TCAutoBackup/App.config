<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="TeamCityServerUrl" value="http://localhost"/>
    <add key="AuthUserName" value="backup"/>
    <add key="AuthPassword" value="pass"/>
    <add key="BackupPath" value="C:\teamcity-backup"/>
    <add key="NumberOfDaysToKeepBackups" value="30"/>
    <add key="FileName" value="tcbackup"/>
    <add key="AddTimestamp" value="true"/>
    <add key="IncludeConfigs" value="true"/>
    <add key="IncludeDatabase" value="true"/>
    <add key="IncludeBuildLogs" value="true"/>
    <add key="IncludePersonalChanges" value="true"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
  </startup>
</configuration>
